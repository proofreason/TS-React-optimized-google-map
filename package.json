{
  "name": "react-optimized-google-map",
  "version": "1.0.0",
  "description": "google maps wrapper optimized for react applications",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "webpack-dev-server --open --config webpack/webpack.dev.js",
    "prod": "webpack --open --config webpack/webpack.prod.js",
    "clean": "rm -rf ./lib & rm -rf ./lib-esm & rm -rf ./dist",
    "build": "npm run clean & ttsc -p ./tsconfig.prod.json & ttsc -p ./tsconfig.prod.json -m es6 --outDir lib-esm",
    "build-all": "npm run build & npm run prod",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
    "tslint-fix": "tslint --fix -c ./tslint.json '{src,develop}/**/*{.ts,.tsx}'",
    "test": "jest --no-cache --config __jest__/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.proofreason.com:e.on/eon-drupal.git"
  },
  "lint-staged": {
    "{src,develop}/**/*.{ts,tsx}": [
      "tslint -c ./tslint.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "Erik Zatloukal <zatloukal.erik@gmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^25.1.4",
    "@zerollup/ts-transform-paths": "^1.7.12",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^25.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv-webpack": "^1.7.0",
    "file-loader": "^5.1.0",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.0",
    "require-dir": "^1.2.0",
    "speed-measure-webpack-plugin": "^1.3.1",
    "terser-webpack-plugin": "^2.3.5",
    "ts-node": "^8.6.2",
    "ts-overlapping-marker-spiderfier": "^1.0.3",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.2.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "utility-types": "^3.10.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@types/googlemaps": "^3.39.3",
    "@types/markerclustererplus": "^2.1.33",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/rewire": "^2.5.28",
    "google-maps-infobox": "^2.0.0",
    "marker-clusterer-plus": "^2.1.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rewire": "^5.0.0",
    "ts-jest": "^25.2.1"
  },
  "keywords": [
    "react",
    "google",
    "maps"
  ]
}
